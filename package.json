{
  "name": "albatross-backend",
  "version": "1.4.2",
  "private": true,
  "scripts": {
    "update-version": "node scripts/update-version.js",
    "prebuild": "npm run update-version",
    "compile": "sass theme/source.scss:theme/backend/albatross-vars.css -I theme/backend --no-source-map & sass theme/source.scss:theme/backend/albatross-vars-minified.css -I theme/backend --no-source-map --style=compressed",
    "compile-styles": "sass src/styles/index.scss:src/styles/main.css --no-source-map",
    "watch-styles": "sass src/styles/index.scss:src/styles/main.css --no-source-map --watch",
    "devBD": "sass --no-source-map theme/debug.scss:%APPDATA%/BetterDiscord/themes/Palettes.theme.css -I theme/backend --no-source-map --watch",
    "devVC": "sass --no-source-map theme/debug.scss:%APPDATA%/Vencord/themes/Palettes.theme.css -I theme/backend --no-source-map --watch",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "author": "",
  "license": "GPL-3.0-only",
  "description": "Albatross Backend",
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@barba/core": "^2.10.3",
    "@iconify-json/majesticons": "^1.2.2",
    "@iconify-json/octicon": "^1.2.10",
    "@iconify-json/qlementine-icons": "^1.2.8",
    "@iconify-json/solar": "^1.2.2",
    "@iconify/json": "^2.2.365",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "astro": "^5.10.1",
    "astro-icon": "^1.1.5",
    "gsap": "^3.13.0",
    "lenis": "^1.3.8",
    "octokit": "^5.0.3",
    "prettier": "^3.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "^1.89.2",
    "tailwindcss": "^4.1.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbatrossCreative/Albatross-Backend.git"
  },
  "bugs": {
    "url": "https://github.com/AlbatrossCreative/Albatross-Backend/issues"
  },
  "homepage": "https://github.com/AlbatrossCreative/Albatross-Backend#readme"
}
