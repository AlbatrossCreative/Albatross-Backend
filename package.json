{
  "name": "albatross-backend",
  "version": "1.4.3",
  "private": true,
  "scripts": {
    "update-version": "node scripts/update-version.js",
    "prebuild": "npm run update-version",
    "compile": "sass theme/source.scss:theme/backend/albatross-vars.css -I theme/backend --no-source-map & sass theme/source.scss:theme/backend/albatross-vars-minified.css -I theme/backend --no-source-map --style=compressed",
    "compile-styles": "sass src/styles/index.scss:src/styles/main.css --no-source-map",
    "watch-styles": "sass src/styles/index.scss:src/styles/main.css --no-source-map --watch",
    "devBD": "sass --no-source-map theme/debug.scss:%APPDATA%/BetterDiscord/themes/Palettes.theme.css -I theme/backend --no-source-map --watch --style=compressed",
    "devVC": "sass --no-source-map theme/debug.scss:%APPDATA%/Vencord/themes/Palettes.theme.css -I theme/backend --no-source-map --watch --style=compressed",
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "author": "",
  "license": "GPL-3.0-only",
  "description": "Albatross Backend",
  "dependencies": {
    "next": "^15.5.2",
    "prettier": "^3.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "^1.89.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbatrossCreative/Albatross-Backend.git"
  },
  "bugs": {
    "url": "https://github.com/AlbatrossCreative/Albatross-Backend/issues"
  },
  "homepage": "https://github.com/AlbatrossCreative/Albatross-Backend#readme",
  "devDependencies": {
    "@types/node": "24.3.1",
    "@types/react": "19.1.12",
    "typescript": "5.9.2"
  }
}
